#standard library
map = \f t -> begin(h=\n->if(eq(n len(t)) n begin(f(take(t n)) h(+(n 1)))) h(0))
foldl = \f i t
        -> begin(h=\n a->if(eq(n len(t))
                            a
                            h(+(n 1) f(a take(t n))))
                 h(0 0))

#test
t = < 1 2 3 4 >
map(\x -> print(+(x 1)) t)
print("foldl:"foldl(+ 0 t))