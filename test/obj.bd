# std
map = \f t
      -> begin(h=\n->if(eq(n len(t)) 1
							          begin(f(take(t n))
							                h(+(n 1))))
				       h(0))
# compound data structure test
Student = \n a 
          -> <n a>

mike = Student("mike" 18)
john = Student("john" 20)
name = \s->take(s 0)
age  = \s->take(s 1)

sayHi = \s -> print("I am " take(s 0)
                    ",my age is " take(s 1))

old = \n s -> set!(s 1 +(take(s 1) n))

years-later = \y->map(old(y) <mike john>)

# test
mike.sayHi
john.sayHi

twe = 2
twe.years-later

